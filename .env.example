# --- Required ---
DATABASE_URL=postgresql+asyncpg://USER:PASS@localhost:5432/agentic
REDIS_URL=redis://localhost:6379/1
REDIS_URL_QUEUE=redis://localhost:6379/2
PUBLIC_BASE_URL=http://localhost:8081

# --- Paths ---
PLUGINS_DIR=./plugins
ARTIFACTS_DIR=./artifacts
SERVICE_FLAGS=*
SOFFICE_BIN=/usr/bin/soffice   # set to your LibreOffice path

# --- Optional/observability ---
ENV=dev
APP_NAME=agentic-dev
LOG_LEVEL=INFO
# Add provider keys ONLY in your local .env (never commit)
# OPENAI_API_KEY=...
# AWS_ACCESS_KEY_ID=...
# AWS_SECRET_ACCESS_KEY=...


# Unsplash or Bing (either or both)
UNSPLASH_ACCESS_KEY=your_unsplash_key
BING_IMAGE_KEY=your_bing_key
