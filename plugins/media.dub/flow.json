{
  "steps": [
    {
      "id": "extract",
      "op": "media.extract_audio",
      "in": {
        "project_id": "{{ context.project_id }}",
        "src_video": "{{ inputs.video }}",
        "sample_rate": 16000
      },
      "out": { "audio": "@audio16k" }
    }
  ],
  "outputs": {
    "dubbed": "{{ inputs.video }}",
    "srt": null,
    "transcript": { "language": "", "segments": [] }
  }
}
